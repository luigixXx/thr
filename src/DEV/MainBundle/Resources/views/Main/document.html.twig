{% extends "DEVMainBundle::layout.html.twig" %}

{% block title%}{{ parent() }} Scoutisme{% endblock%}
{% block meta %}
<meta name="Description" content="Les documents administratifs nécessaires pour l'inscription et l'assurance de vos enfants lors des activité">
<meta name="Keywords" content="Documents, inscriptions, assurance, administratifs">
<meta name="Subject" content="Les Documents administratifs">
<meta name="Revisit-After" content="15 days">
<meta name="Robots" content="all">
<meta name="Rating" content="general">
<meta name="Distribution" content="global">
<meta name="Category" content="sports">
{% endblock %}
{%block body%}
<ol class="breadcrumb">
  <li><a href="{{path('dev_main_homepage')}}">Accueil</a></li>
  <li>Document</li>
</ol>
<div class="col-md-8">
	<div class="well-thr">
		<legend>
			Les documents administratifs
		</legend>
		<table class="table">
			{%for doc in docs%}
			<tr>
				<td><a href="{{asset(doc.webpath)}}"><i class="glyphicon glyphicon-download"></i> {{doc.nom}}</a></td>
			</tr>
			{%endfor%}
		</table>
	</div>
</div>
<script>
	$(".fcbox").fancybox({
		'hideOnContentClick' : true,
		'padding' : 0,
		'overlayColor' : '#D3D3D3',
		'transitionIn' : 'elastic',
		'transitionOut' : 'elastic',
		'overlayOpacity' : 0.7,
		'zoomSpeedIn' : 300,
		'zoomSpeedOut' : 300,
		'width' : 950,
		'height' : 600,
		'type' : 'iframe'
	}); 
</script>
{%endblock%}
